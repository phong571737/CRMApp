<?xml version='1.0' encoding='utf-8'?>
<resources>
    <style name="SummaryRow">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingVertical">8dp</item>
    </style>

    <style name="SummaryLabel">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#9E9E9E</item>
    </style>

    <style name="SummarySpacer">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="SummaryValue">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="SummaryEditIcon">
        <item name="android:layout_width">16dp</item>
        <item name="android:layout_height">16dp</item>
        <item name="android:src">@drawable/ic_circum_edit</item>
        <item name="android:layout_marginStart">8dp</item>
    </style>

    
    <style name="OverviewCard" parent="Widget.MaterialComponents.CardView">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_columnWeight">1</item>
        <item name="android:layout_margin">4dp</item>
        <item name="cardCornerRadius">8dp</item>
        <item name="cardElevation">2dp</item>
    </style>

    
    <style name="OverviewCard.Dark" parent="OverviewCard">
        <item name="android:backgroundTint">@color/blue</item>
    </style>

    
    <style name="OverviewCard.Light" parent="OverviewCard">
        <item name="android:backgroundTint">#EFF8FF</item>
    </style>

    <style name="OverviewCardContent">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="android:paddingStart">4dp</item>
        <item name="android:paddingEnd">4dp</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <style name="OverviewCardValue">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/blue</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>

    <style name="OverviewCardLabel">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">@color/blue</item>
        <item name="android:layout_marginTop">4dp</item>
    </style>

    <style name="BottomSheetActionItem" parent="Widget.AppCompat.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">16dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:drawablePadding">16dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">?attr/selectableItemBackground</item>
        <item name="android:drawableTint">@color/blue</item>
    </style>

    
    <style name="StatItem">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="StatValue">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">"#3B72F2"</item>
    </style>
    <style name="StatLabel">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="VerticalDivider">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">#F0F0F0</item>
    </style>

    
    <style name="DetailSectionContainer">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@color/white</item>
        <item name="android:padding">16dp</item>
        <item name="android:layout_marginTop">8dp</item>
    </style>
    <style name="DetailSectionHeader">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingBottom">8dp</item>
    </style>
    <style name="DetailSectionTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="DetailSectionToggleIcon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/ic_arrow_up</item>
        <item name="android:layout_alignParentEnd">true</item>
    </style>

    <style name="DetailRowLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingVertical">8dp</item>
    </style>

    <style name="DetailLabel">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentStart">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textColor">#9E9E9E</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="DetailValue">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentEnd">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">end</item>
        <item name="android:layout_marginStart">100dp</item>
    </style>

    <style name="DetailValue.Blue">
        <item name="android:textColor">@color/blue</item>
    </style>

    <style name="DetailSectionContainer.NoTopMargin">
        <item name="android:layout_marginTop">0dp</item>
    </style>

    <style name="DetailRowLayout.Vertical" parent="DetailRowLayout">
        <item name="android:orientation">vertical</item>
    </style>

    <style name="DetailValue.FullWidth" parent="DetailValue">
        <item name="android:layout_alignParentEnd">false</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:gravity">start</item>
    </style>
<style name="NavIndicatorStyle">
        <item name="android:color">#E0E7FF</item> 
        <item name="android:alpha">1.0</item>
        <item name="shapeAppearanceOverlay">@style/ShapeAppearance.Material3.Corner.Full</item>
    </style>
    <style name="Widget.SO.TextInputLayout.Outlined" parent="Widget.Material3.TextInputLayout.OutlinedBox">
        <item name="boxBackgroundMode">outline</item>
        <item name="boxBackgroundColor">@color/card_bg</item>

        
        <item name="boxStrokeColor">@color/til_stroke_selector</item>
        <item name="boxStrokeWidth">1dp</item>
        <item name="boxStrokeWidthFocused">2dp</item>

        
        <item name="boxCornerRadiusTopStart">14dp</item>
        <item name="boxCornerRadiusTopEnd">14dp</item>
        <item name="boxCornerRadiusBottomStart">14dp</item>
        <item name="boxCornerRadiusBottomEnd">14dp</item>

        
        <item name="hintTextColor">@color/til_hint_selector</item>
        <item name="startIconTint">@color/text_secondary</item>
        <item name="endIconTint">@color/text_secondary</item>
    </style>

    <style name="Widget.SO.TextInputLayout.Outlined.Dropdown" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu">
        <item name="boxBackgroundMode">outline</item>
        <item name="boxBackgroundColor">@color/card_bg</item>
        <item name="boxStrokeColor">@color/til_stroke_selector</item>
        <item name="boxStrokeWidth">1dp</item>
        <item name="boxStrokeWidthFocused">2dp</item>
        <item name="boxCornerRadiusTopStart">14dp</item>
        <item name="boxCornerRadiusTopEnd">14dp</item>
        <item name="boxCornerRadiusBottomStart">14dp</item>
        <item name="boxCornerRadiusBottomEnd">14dp</item>
        <item name="hintTextColor">@color/til_hint_selector</item>
        <item name="startIconTint">@color/text_secondary</item>
        <item name="endIconTint">@color/text_secondary</item>
    </style>
    
    <style name="SoInfoLabel">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/so_text_secondary</item>
    </style>

    
    <style name="SoInfoValue">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textStyle">bold</item>
    </style>

    
    <style name="SoInfoLink" parent="SoInfoValue">
        <item name="android:textColor">@color/so_link</item>
    </style>

    
    <style name="SoDivider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/so_stroke</item>
    </style>

    
    <style name="Widget.App.Card" parent="Widget.MaterialComponents.CardView">

    <item name="cardBackgroundColor">@color/white</item>
        <item name="strokeColor">@color/so_stroke</item>
        <item name="strokeWidth">1dp</item>
        <item name="cardCornerRadius">16dp</item>
        <item name="cardUseCompatPadding">true</item>
    </style>




</resources>